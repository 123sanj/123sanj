a =10
b =4

print ("Addition")
print ("the value of",a, "+" ,b, "is", a+b )
print ("Subtraction")
print ("the value of",a, "-" ,b, "is", a-b )
print ("Multiplication")
print ("the value of",a, "*" ,b, "is", a*b )
print ("Division")
print ("the value of",a, "/" ,b, "is", a/b )
print("Modulus")
print ("the value of",a, "%" ,b, "is", a%b )
print ("Exponentiation") 
print ("The value of",a,"**",b, "is", a**b)
print("Floor division")
print ("the value of",a, "//",b,"is",a//b)
